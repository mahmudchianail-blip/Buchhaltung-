    <div class="optispar-header-wrapper optispar-animate">
      <div class="optispar-header" data-optispar-header>
        {%- if section.settings.menu != blank -%}
          <div class="optispar-mobile-menu">
            {% render 'header-drawer' %}
          </div>
        {%- endif -%}

        <div class="optispar-header__logo">
          {%- if request.page_type == 'index' -%}
            <h1 class="header__heading">
          {%- endif -%}
