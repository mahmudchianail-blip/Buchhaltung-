  }

  .optispar-mobile-menu .header__icon:hover,
  .optispar-mobile-menu .header__icon:focus-visible {
    background: rgba(255, 255, 255, 0.08);
    box-shadow: 0 12px 24px rgba(31, 63, 125, 0.3);
    transform: translateY(-1px);
    outline: none;
  }

  .optispar-mobile-menu .header__icon svg {
    width: 1.375rem;
    height: 1.375rem;
  }

  .optispar-icon-btn,
  .optispar-search__submit,
  .optispar-search__close {
    position: relative;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    width: 3rem;
    height: 3rem;
    border-radius: 50%;
    background: transparent;
    border: 1px solid rgba(255, 255, 255, 0.45);
    box-shadow: 0 8px 16px rgba(31, 63, 125, 0.25);
    color: #fff;
    transition: background-color 0.2s ease, box-shadow 0.2s ease, transform 0.2s ease;
  }

  .optispar-icon-btn svg,
  .optispar-search__submit svg,
  .optispar-search__close svg {
    width: 1.375rem;
    height: 1.375rem;
    pointer-events: none;
  }

  .optispar-icon-btn:hover,
  .optispar-icon-btn:focus-visible,
  .optispar-search__submit:hover,
  .optispar-search__submit:focus-visible,
  .optispar-search__close:hover,
  .optispar-search__close:focus-visible {
    background: rgba(255, 255, 255, 0.08);
    box-shadow: 0 12px 24px rgba(31, 63, 125, 0.3);
    transform: translateY(-1px);
    outline: none;
  }

  .optispar-search {
    position: relative;
    display: flex;
    align-items: center;
    flex-shrink: 0;
    min-width: var(--optispar-action-size);
  }

  .optispar-search__toggle {
    width: 100%;
    height: 100%;
    flex-shrink: 0;
  }

  .optispar-search__form {
    display: flex;
    align-items: center;
    gap: 0.75rem;
    width: 0;
    opacity: 0;
    pointer-events: none;
    overflow: hidden;
    margin-left: 0;
    padding: 0.5rem 0.75rem 0.5rem 1rem;
    border-radius: 9999px;
    background: rgba(8, 18, 40, 0.9);
    border: 1px solid rgba(255, 255, 255, 0.35);
    box-shadow: 0 18px 32px rgba(12, 28, 60, 0.35);
    transition: width 0.32s ease, opacity 0.3s ease, margin 0.3s ease;
  }

  .optispar-search.is-open .optispar-search__toggle {
    display: none;
  }

  .optispar-search.is-open .optispar-search__form {
    width: clamp(16rem, 36vw, 24rem);
    margin-left: 0.75rem;
    opacity: 1;
    pointer-events: auto;
  }

  .optispar-search__input {
    flex: 1;
    min-width: 0;
    border: none;
    background: transparent;
    color: #fff;
    font-size: clamp(0.9rem, 1.15vw, 1.05rem);
    line-height: 1.4;
    padding: 0.3rem 0;
