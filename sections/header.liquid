    height: 100%;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    border-radius: 50%;
    background: transparent;
    border: 1px solid rgba(255, 255, 255, 0.45);
    box-shadow: 0 8px 16px rgba(31, 63, 125, 0.25);
    color: #fff;
    transition: background-color 0.2s ease, box-shadow 0.2s ease, transform 0.2s ease;
  }

  .optispar-action-cluster .optispar-icon-btn:hover,
  .optispar-action-cluster .optispar-icon-btn:focus-visible,
  .optispar-action-cluster .optispar-search__toggle:hover,
  .optispar-action-cluster .optispar-search__toggle:focus-visible,
  .optispar-action-cluster .optispar-search__submit:hover,
  .optispar-action-cluster .optispar-search__submit:focus-visible,
  .optispar-action-cluster .optispar-search__close:hover,
  .optispar-action-cluster .optispar-search__close:focus-visible {
    background: rgba(255, 255, 255, 0.08);
    box-shadow: 0 12px 24px rgba(31, 63, 125, 0.3);
    transform: translateY(-1px);
    outline: none;
  }

