  .optispar-search__input:focus {
    outline: none;
  }

  .optispar-search__submit,
  .optispar-search__close {
    border: none;
    color: #fff;
    cursor: pointer;
  }

  .optispar-icon-btn .cart-count-bubble {
    position: absolute;
    top: -0.32rem;
    right: -0.32rem;
    min-width: 1.1rem;
    height: 1.1rem;
    padding: 0 0.3rem;
    border-radius: 999px;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #ef4444;
    color: #fff;
    font-size: 0.58rem;
    font-weight: 700;
    border: 1.5px solid rgba(14, 56, 118, 0.82);
  }

  .optispar-localization {
    display: flex;
    gap: 0.35rem;
  }

  .shopify-section-header-sticky .optispar-header-wrapper {
    padding: clamp(0.2rem, 1vw, 0.55rem) clamp(0.5rem, 1.8vw, 1.1rem);
  }

  .shopify-section-header-sticky .optispar-header {
    min-height: clamp(2.1rem, 4.6vw, 3.2rem);
    padding: clamp(0.2rem, 1.15vw, 0.65rem) clamp(0.6rem, 1.8vw, 1.1rem);
    box-shadow: 0 12px 26px rgba(12, 54, 120, 0.24);
    background: linear-gradient(
      135deg,
      rgba(17, 57, 130, 0.82),
      rgba(41, 124, 204, 0.68)
    );
    border-color: rgba(160, 206, 255, 0.38);
  }

  .shopify-section-header-sticky .optispar-nav__indicator {
    height: clamp(2.05rem, 2.6vw, 2.35rem);
  }

  .shopify-section-header-sticky .optispar-nav__link {
    padding-block: clamp(0.32rem, 0.85vw, 0.48rem);
  }

  .shopify-section-header-sticky .optispar-header__actions,
  .shopify-section-header-sticky .optispar-search__form,
  .shopify-section-header-sticky .optispar-icon-btn,
