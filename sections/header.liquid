  </header>

  <script>
    (function () {
      if (typeof document === 'undefined') return;

      const initHeader = (root) => {
        const header = root.querySelector('[data-optispar-header]');
        if (!header || header.dataset.optisparInit === 'true') {
          return;
        }

        header.dataset.optisparInit = 'true';

        const nav = header.querySelector('[data-optispar-nav]');
        if (nav) {
          const indicator = nav.querySelector('.optispar-nav__indicator');
          const links = Array.from(nav.querySelectorAll('.optispar-nav__link'));
          let activeLink = links.find((link) => link.classList.contains('optispar-nav__link--active')) || links[0];

          const updateIndicator = (link) => {
            if (!indicator || !link) {
              return;
            }

            const navRect = nav.getBoundingClientRect();
            const linkRect = link.getBoundingClientRect();
            indicator.style.width = `${linkRect.width}px`;
            indicator.style.left = `${linkRect.left - navRect.left}px`;
            indicator.style.opacity = '1';
          };

          if (activeLink) {
            updateIndicator(activeLink);
          }

          const handleResize = () => {
            if (activeLink && document.body.contains(activeLink)) {
              updateIndicator(activeLink);
            }
          };

          window.addEventListener('resize', handleResize);
          header.optisparResizeHandler = handleResize;

          links.forEach((link) => {
            link.addEventListener('mouseenter', () => updateIndicator(link));
            link.addEventListener('focus', () => updateIndicator(link));
            link.addEventListener('click', () => {
              activeLink = link;
              updateIndicator(activeLink);
            });
          });

          nav.addEventListener('mouseleave', () => updateIndicator(activeLink));
        }

        const setGlowPosition = (point) => {
          if (!point) {
            return;
          }

          const rect = header.getBoundingClientRect();
          const x = Math.max(0, Math.min(rect.width, point.clientX - rect.left));
          const y = Math.max(0, Math.min(rect.height, point.clientY - rect.top));

          header.style.setProperty('--optispar-mouse-x', `${x}px`);
          header.style.setProperty('--optispar-mouse-y', `${y}px`);
        };

        const handleGlowMouseMove = (event) => {
          setGlowPosition(event);
        };

        const handleGlowTouchMove = (event) => {
          const touch = event.touches && event.touches[0];
          if (touch) {
            setGlowPosition(touch);
          }
        };

        const handleGlowLeave = () => {
          header.style.setProperty('--optispar-mouse-x', '50%');
          header.style.setProperty('--optispar-mouse-y', '50%');
        };

        header.addEventListener('mousemove', handleGlowMouseMove);
        header.addEventListener('mouseleave', handleGlowLeave);
        header.addEventListener('touchmove', handleGlowTouchMove, { passive: true });
        header.addEventListener('touchend', handleGlowLeave, { passive: true });
        header.addEventListener('touchcancel', handleGlowLeave, { passive: true });

        header.optisparGlowHandlers = {
          mousemove: handleGlowMouseMove,
          mouseleave: handleGlowLeave,
          touchmove: handleGlowTouchMove,
          touchend: handleGlowLeave,
          touchcancel: handleGlowLeave,
        };

        const searchContainer = header.querySelector('[data-optispar-search]');
        if (searchContainer) {
          const toggleButton = searchContainer.querySelector('[data-optispar-search-toggle]');
          const closeButton = searchContainer.querySelector('[data-optispar-search-close]');
          const form = searchContainer.querySelector('[data-optispar-search-form]');
          const input = form ? form.querySelector('.optispar-search__input') : null;

          const openSearch = () => {
            searchContainer.classList.add('is-open');
            if (toggleButton) {
              toggleButton.setAttribute('aria-expanded', 'true');
            }
            window.requestAnimationFrame(() => {
              input && input.focus();
            });
          };

          const closeSearch = () => {
            searchContainer.classList.remove('is-open');
            if (toggleButton) {
              toggleButton.setAttribute('aria-expanded', 'false');
            }
          };

          if (toggleButton) {
            toggleButton.addEventListener('click', (event) => {
              event.preventDefault();
              openSearch();
            });
          }

          if (closeButton) {
            closeButton.addEventListener('click', (event) => {
              event.preventDefault();
              closeSearch();
              toggleButton && toggleButton.focus();
            });
          }

          if (input) {
            input.addEventListener('keydown', (event) => {
              if (event.key === 'Escape') {
                closeSearch();
                toggleButton && toggleButton.focus();
              }
            });
          }

          const handleDocumentInteraction = (event) => {
            if (!searchContainer.contains(event.target)) {
              closeSearch();
            }
          };

          document.addEventListener('mousedown', handleDocumentInteraction);
          document.addEventListener('touchstart', handleDocumentInteraction, { passive: true });
          header.optisparSearchHandler = handleDocumentInteraction;
        }
      };

      const destroyHeader = (root) => {
        const header = root.querySelector('[data-optispar-header]');
        if (!header || header.dataset.optisparInit !== 'true') {
          return;
        }

        header.dataset.optisparInit = 'false';

        if (header.optisparResizeHandler) {
          window.removeEventListener('resize', header.optisparResizeHandler);
          delete header.optisparResizeHandler;
        }

        if (header.optisparSearchHandler) {
          document.removeEventListener('mousedown', header.optisparSearchHandler);
          document.removeEventListener('touchstart', header.optisparSearchHandler);
          delete header.optisparSearchHandler;
        }

        if (header.optisparGlowHandlers) {
          const handlers = header.optisparGlowHandlers;
          header.removeEventListener('mousemove', handlers.mousemove);
          header.removeEventListener('mouseleave', handlers.mouseleave);
          header.removeEventListener('touchmove', handlers.touchmove);
          header.removeEventListener('touchend', handlers.touchend);
          header.removeEventListener('touchcancel', handlers.touchcancel);
          delete header.optisparGlowHandlers;
        }
      };

      const initContext = (context) => {
        if (!context) return;
        initHeader(context);
      };

      const ready = () => initContext(document);

      if (document.readyState === 'loading') {
        document.addEventListener('DOMContentLoaded', ready, { once: true });
      } else {
        ready();
      }

      document.addEventListener('shopify:section:load', (event) => initContext(event.target));
      document.addEventListener('shopify:section:unload', (event) => destroyHeader(event.target));
    })();
  </script>
</{{ header_tag }}>

