      header-drawer {
        display: none;
      }
    }
  {%- endif -%}

  .menu-drawer-container {
    display: flex;
  }

  .list-menu {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  .list-menu--inline {
    display: inline-flex;
    flex-wrap: wrap;
  }

  summary.list-menu__item {
    padding-right: 2.7rem;
  }

  .list-menu__item {
    display: flex;
    align-items: center;
    line-height: calc(1 + 0.3 / var(--font-body-scale));
  }

  .list-menu__item--link {
    text-decoration: none;
    padding-bottom: 1rem;
    padding-top: 1rem;
    line-height: calc(1 + 0.8 / var(--font-body-scale));
  }

  @media screen and (min-width: 750px) {
    .list-menu__item--link {
      padding-bottom: 0.5rem;
      padding-top: 0.5rem;
    }
  }

  @keyframes optispar-fade-in-up {
    from {
      opacity: 0;
      transform: translate3d(0, 20px, 0);
    }

    to {
      opacity: 1;
      transform: translate3d(0, 0, 0);
    }
  }

  .optispar-animate {
    animation: optispar-fade-in-up 0.8s ease-out forwards;
    opacity: 0;
  }

  .header-wrapper,
  .header-wrapper .page-width {
    width: 100%;
    max-width: none;
  }

  .header-wrapper {
    background: transparent;
  }

  .header {
    display: block;
    max-width: none;
    margin: 0;
    width: 100%;
  }

  .header.page-width {
    max-width: none;
    width: 100%;
    padding-left: 0;
    padding-right: 0;
  }

  .header:not(.drawer-menu).page-width {
    padding-left: 0;
    padding-right: 0;
  }

  @media screen and (min-width: 750px) {
    .header.page-width {
      padding-left: 0;
      padding-right: 0;
    }
  }

  @media screen and (min-width: 990px) {
    .header:not(.drawer-menu).page-width {
      padding-left: 0;
      padding-right: 0;
    }
  }

  .optispar-header-wrapper {
    position: relative;
    display: flex;
    justify-content: center;
    width: 100%;
    padding: clamp(0.25rem, 1.6vw, 0.75rem) clamp(0.55rem, 2.2vw, 1.35rem);
    box-sizing: border-box;
    pointer-events: none;
  }

  .optispar-header {
    --optispar-mouse-x: 50%;
    --optispar-mouse-y: 50%;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: min(70rem, 100%);
    min-height: clamp(2.4rem, 5.4vw, 3.75rem);
    padding: clamp(0.25rem, 1.45vw, 0.8rem) clamp(0.7rem, 2.1vw, 1.3rem);
    border-radius: clamp(1.35rem, 3.6vw, 3.1rem);
    background: linear-gradient(
      135deg,
      rgba(20, 70, 148, 0.78),
      rgba(52, 142, 221, 0.62)
    );
    border: 1px solid rgba(160, 206, 255, 0.38);
    box-shadow: 0 14px 30px rgba(12, 54, 120, 0.26);
    backdrop-filter: blur(18px);
    overflow: visible;
    pointer-events: auto;
  }

  .optispar-header::before {
    content: '';
    position: absolute;
    inset: -1px;
    border-radius: inherit;
    background: radial-gradient(
      circle 260px at var(--optispar-mouse-x) var(--optispar-mouse-y),
      rgba(143, 205, 255, 0.38),
      rgba(143, 205, 255, 0)
    );
    opacity: 0;
    transition: opacity 0.4s ease;
    pointer-events: none;
  }

  .optispar-header:hover::before,
  .optispar-header:focus-within::before {
    opacity: 1;
  }

  .optispar-header__logo,
  .optispar-header__actions {
    position: relative;
    z-index: 2;
  }

  .optispar-header__logo {
    display: flex;
    align-items: center;
    margin-left: clamp(0.75rem, 2.8vw, 2.25rem);
    margin-right: clamp(1rem, 3.4vw, 2.6rem);
  }

  .optispar-header__logo a {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    padding: clamp(0.05rem, 0.4vw, 0.2rem) clamp(0.8rem, 2.4vw, 1.6rem);
    color: #f6f9ff;
    font-size: clamp(1.35rem, 2.6vw, 1.95rem);
    font-weight: 700;
    letter-spacing: 0.085em;
    line-height: 1.08;
    text-decoration: none;
    text-transform: uppercase;
    text-shadow: 0 10px 24px rgba(12, 54, 120, 0.38), 0 2px 6px rgba(255, 255, 255, 0.28);
    transition: color 0.3s ease, transform 0.3s ease, text-shadow 0.3s ease;
  }

  .optispar-header__logo a:hover,
  .optispar-header__logo a:focus-visible {
    color: #f3f7ff;
    transform: translateY(-1px);
    outline: none;
  }

  .optispar-header__nav {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate3d(-50%, -50%, 0);
    z-index: 1;
  }

  .optispar-nav__list {
    display: flex;
    align-items: center;
    gap: clamp(0.25rem, 0.85vw, 0.5rem);
    color: rgba(235, 244, 255, 0.82);
    padding: 0 clamp(0.35rem, 1.2vw, 0.85rem) 0 clamp(0.85rem, 2.6vw, 1.75rem);
    margin: 0;
    list-style: none;
    position: relative;
  }

  .optispar-nav__item {
    position: relative;
  }

  .optispar-nav__link {
    display: inline-block;
    padding: clamp(0.38rem, 0.95vw, 0.58rem) clamp(0.9rem, 2.05vw, 1.35rem);
    font-size: clamp(0.9rem, 1.2vw, 1.05rem);
