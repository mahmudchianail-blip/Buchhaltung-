  .optispar-nav__link:hover,
  .optispar-nav__link:focus-visible {
    color: #fff;
    outline: none;
  }

  .optispar-nav__link--active {
    color: #fff;
  }

  .optispar-nav__divider {
    user-select: none;
    color: rgba(216, 230, 255, 0.35);
  }

  .optispar-nav__indicator {
    position: absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
    height: clamp(2.05rem, 2.6vw, 2.45rem);
    border-radius: 9999px;
