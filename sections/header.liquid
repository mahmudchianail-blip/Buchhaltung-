    border-color: rgba(160, 206, 255, 0.48);
    box-shadow:
      inset 0 1px 0 rgba(250, 253, 255, 0.32),
      0 10px 26px rgba(12, 54, 120, 0.26);
  }

  @media screen and (max-width: 989px) {
