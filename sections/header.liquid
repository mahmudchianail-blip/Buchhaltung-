    transition: left 0.4s ease, width 0.4s ease, opacity 0.3s ease;
    opacity: 0;
    pointer-events: none;
  }

  .optispar-header__actions {
    display: flex;
    align-items: center;
    gap: clamp(0.28rem, 1vw, 0.55rem);
  }

  .optispar-action-cluster {
    --optispar-action-size: 3rem;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    gap: 0.75rem;
  }

  .optispar-action-cluster__buttons {
    display: flex;
    align-items: center;
    justify-content: flex-end;
    gap: 0.75rem;
  }

  .optispar-action-cluster__buttons > * {
    display: flex;
    align-items: center;
    justify-content: center;
    width: var(--optispar-action-size);
    height: var(--optispar-action-size);
    border-radius: 50%;
  }

  .optispar-action-cluster__buttons > .optispar-search {
    position: relative;
    width: var(--optispar-action-size);
    flex: 0 0 var(--optispar-action-size);
    transition: width 0.3s ease;
    overflow: visible;
    border: none;
    background: transparent;
    box-shadow: none;
