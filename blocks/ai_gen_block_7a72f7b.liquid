{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-parallax-image-text-{{ ai_gen_id }} {
    display: flex;
    align-items: {{ block.settings.vertical_alignment }};
    gap: {{ block.settings.spacing }}px;
    padding: {{ block.settings.padding_top }}px {{ block.settings.padding_horizontal }}px {{ block.settings.padding_bottom }}px;
    background: {{ block.settings.background_color }};
    max-width: {{ block.settings.container_width }}px;
    margin: 0 auto;
    overflow: hidden;
    position: relative;
  }

  .ai-parallax-image-text-{{ ai_gen_id }}--reverse {
    flex-direction: row-reverse;
  }

  .ai-parallax-image-text__image-wrapper-{{ ai_gen_id }} {
    flex: 0 0 {{ block.settings.image_width }}%;
    position: relative;
    overflow: hidden;
    border-radius: {{ block.settings.image_border_radius }}px;
    will-change: transform;
    box-shadow: {{ block.settings.image_shadow_x }}px {{ block.settings.image_shadow_y }}px {{ block.settings.image_shadow_blur }}px rgba(0, 0, 0, {{ block.settings.image_shadow_opacity | divided_by: 100.0 }});
  }

  .ai-parallax-image-text__image-{{ ai_gen_id }} {
    width: 100%;
    height: auto;
    display: block;
    object-fit: cover;
    filter: brightness({{ block.settings.image_brightness }}%) contrast({{ block.settings.image_contrast }}%) saturate({{ block.settings.image_saturation }}%);
    transition: filter 0.3s ease;
  }

  .ai-parallax-image-text__image-placeholder-{{ ai_gen_id }} {
    width: 100%;
    aspect-ratio: {{ block.settings.image_aspect_ratio }};
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
  }

  .ai-parallax-image-text__image-placeholder-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-parallax-image-text__empty-state-{{ ai_gen_id }} {
    position: absolute;
    bottom: 20px;
    left: 50%;
    transform: translateX(-50%);
    padding: 12px 20px;
    font-size: 14px;
    color: #666;
    text-align: center;
    pointer-events: none;
  }

  .ai-parallax-image-text__content-{{ ai_gen_id }} {
    flex: 1;
    padding: {{ block.settings.content_padding }}px;
    will-change: transform;
    text-align: {{ block.settings.text_alignment }};
  }

  .ai-parallax-image-text__heading-{{ ai_gen_id }} {
    font-size: {{ block.settings.heading_size }}px;
    color: {{ block.settings.heading_color }};
    margin: 0 0 {{ block.settings.heading_spacing }}px;
    font-weight: {{ block.settings.heading_weight }};
    line-height: {{ block.settings.heading_line_height }};
    letter-spacing: {{ block.settings.heading_letter_spacing }}px;
    text-transform: {{ block.settings.heading_transform }};
  }

  .ai-parallax-image-text__text-{{ ai_gen_id }} {
    font-size: {{ block.settings.text_size }}px;
    color: {{ block.settings.text_color }};
    line-height: {{ block.settings.text_line_height }};
    margin: 0 0 {{ block.settings.text_spacing }}px;
    font-weight: {{ block.settings.text_weight }};
    letter-spacing: {{ block.settings.text_letter_spacing }}px;
  }

  .ai-parallax-image-text__button-{{ ai_gen_id }} {
    display: inline-block;
    padding: {{ block.settings.button_padding_vertical }}px {{ block.settings.button_padding_horizontal }}px;
    background-color: {{ block.settings.button_bg_color }};
    color: {{ block.settings.button_text_color }};
    text-decoration: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    font-size: {{ block.settings.button_text_size }}px;
    font-weight: {{ block.settings.button_font_weight }};
    letter-spacing: {{ block.settings.button_letter_spacing }}px;
    text-transform: {{ block.settings.button_text_transform }};
    transition: all 0.3s ease;
    border: {{ block.settings.button_border_width }}px solid {{ block.settings.button_border_color }};
    cursor: pointer;
    box-shadow: {{ block.settings.button_shadow_x }}px {{ block.settings.button_shadow_y }}px {{ block.settings.button_shadow_blur }}px rgba(0, 0, 0, {{ block.settings.button_shadow_opacity | divided_by: 100.0 }});
  }

  .ai-parallax-image-text__button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_bg_color }};
    color: {{ block.settings.button_hover_text_color }};
    border-color: {{ block.settings.button_hover_border_color }};
    transform: translateY({{ block.settings.button_hover_lift }}px) scale({{ block.settings.button_hover_scale | divided_by: 100.0 }});
    box-shadow: {{ block.settings.button_hover_shadow_x }}px {{ block.settings.button_hover_shadow_y }}px {{ block.settings.button_hover_shadow_blur }}px rgba(0, 0, 0, {{ block.settings.button_hover_shadow_opacity | divided_by: 100.0 }});
  }

  @media screen and (max-width: 749px) {
    .ai-parallax-image-text-{{ ai_gen_id }} {
      flex-direction: column;
      gap: {{ block.settings.spacing | times: 0.7 }}px;
      padding: {{ block.settings.padding_top | times: 0.7 }}px {{ block.settings.padding_horizontal | times: 0.7 }}px {{ block.settings.padding_bottom | times: 0.7 }}px;
    }

    .ai-parallax-image-text-{{ ai_gen_id }}--reverse {
      flex-direction: column;
    }

    .ai-parallax-image-text__image-wrapper-{{ ai_gen_id }} {
      flex: 0 0 100%;
      width: 100%;
    }

    .ai-parallax-image-text__content-{{ ai_gen_id }} {
      padding: {{ block.settings.content_padding | times: 0.7 }}px;
      text-align: {{ block.settings.mobile_text_alignment }};
    }

    .ai-parallax-image-text__heading-{{ ai_gen_id }} {
      font-size: {{ block.settings.heading_size | times: 0.8 }}px;
    }

    .ai-parallax-image-text__text-{{ ai_gen_id }} {
      font-size: {{ block.settings.text_size | times: 0.9 }}px;
    }
  }
{% endstyle %}

<parallax-scroll-{{ ai_gen_id }}
  class="ai-parallax-image-text-{{ ai_gen_id }} {% if block.settings.image_position == 'right' %}ai-parallax-image-text-{{ ai_gen_id }}--reverse{% endif %}"
  data-parallax-speed="{{ block.settings.parallax_speed }}"
  data-text-speed="{{ block.settings.text_parallax_speed }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-parallax-image-text__image-wrapper-{{ ai_gen_id }}" data-parallax-image>
    {% if block.settings.image %}
      <img
        src="{{ block.settings.image | image_url: width: 1200 }}"
        alt="{{ block.settings.image.alt | escape }}"
        loading="lazy"
        width="{{ block.settings.image.width }}"
        height="{{ block.settings.image.height }}"
        class="ai-parallax-image-text__image-{{ ai_gen_id }}"
      >
    {% else %}
      <div class="ai-parallax-image-text__image-placeholder-{{ ai_gen_id }}">
        {{ 'image' | placeholder_svg_tag }}
        <div class="ai-parallax-image-text__empty-state-{{ ai_gen_id }}">
          Bild hinzufügen
        </div>
      </div>
    {% endif %}
  </div>

  <div class="ai-parallax-image-text__content-{{ ai_gen_id }}" data-parallax-text>
    {% if block.settings.heading != blank %}
      <h2 class="ai-parallax-image-text__heading-{{ ai_gen_id }}">{{ block.settings.heading }}</h2>
    {% endif %}

    {% if block.settings.text != blank %}
      <div class="ai-parallax-image-text__text-{{ ai_gen_id }}">{{ block.settings.text }}</div>
    {% endif %}

    {% if block.settings.button_text != blank and block.settings.button_link != blank %}
      <a
        href="{{ block.settings.button_link }}"
        class="ai-parallax-image-text__button-{{ ai_gen_id }}"
      >
        {{ block.settings.button_text }}
      </a>
    {% endif %}
  </div>
</parallax-scroll-{{ ai_gen_id }}>

<script>
  (function() {
    class ParallaxScroll{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.imageWrapper = this.querySelector('[data-parallax-image]');
        this.textWrapper = this.querySelector('[data-parallax-text]');
        this.parallaxSpeed = parseFloat(this.dataset.parallaxSpeed) || 1.5;
        this.textSpeed = parseFloat(this.dataset.textSpeed) || 1;
        this.ticking = false;
        this.lastScrollY = 0;
      }

      connectedCallback() {
        this.setupParallax();
      }

      setupParallax() {
        if (!this.imageWrapper && !this.textWrapper) return;

        const handleScroll = () => {
          this.lastScrollY = window.scrollY;
          
          if (!this.ticking) {
            window.requestAnimationFrame(() => {
              this.updateParallax();
              this.ticking = false;
            });
            this.ticking = true;
          }
        };

        window.addEventListener('scroll', handleScroll, { passive: true });
        this.updateParallax();
        
        this.disconnectCallback = () => {
          window.removeEventListener('scroll', handleScroll);
        };
      }

      updateParallax() {
        const rect = this.getBoundingClientRect();
        const windowHeight = window.innerHeight;
        
        if (rect.top < windowHeight && rect.bottom > 0) {
          const scrollProgress = (windowHeight - rect.top) / (windowHeight + rect.height);
          const centerOffset = scrollProgress - 0.5;
          
          if (this.imageWrapper) {
            const imageTranslate = centerOffset * 200 * this.parallaxSpeed;
            this.imageWrapper.style.transform = `translateX(${imageTranslate}px)`;
          }
          
          if (this.textWrapper) {
            const textTranslate = centerOffset * -150 * this.textSpeed;
            this.textWrapper.style.transform = `translateX(${textTranslate}px)`;
          }
        }
      }

      disconnectedCallback() {
        if (this.disconnectCallback) {
          this.disconnectCallback();
        }
      }
    }

    customElements.define('parallax-scroll-{{ ai_gen_id }}', ParallaxScroll{{ ai_gen_id }});
  })();
</script>

