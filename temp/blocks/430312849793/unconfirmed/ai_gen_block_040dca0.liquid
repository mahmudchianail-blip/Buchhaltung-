{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-why-choose-us-{{ ai_gen_id }} {
    padding: {{ block.settings.padding_top }}px 20px {{ block.settings.padding_bottom }}px;
    background: {{ block.settings.background_gradient }};
    position: relative;
    overflow: hidden;
  }

  .ai-why-choose-us-bg-decoration-{{ ai_gen_id }} {
    position: absolute;
    width: 400px;
    height: 400px;
    border-radius: 50%;
    background: linear-gradient(135deg, {{ block.settings.accent_color_1 }}15, {{ block.settings.accent_color_2 }}15);
    filter: blur(80px);
    pointer-events: none;
  }

  .ai-why-choose-us-bg-decoration-{{ ai_gen_id }}:nth-child(1) {
    top: -200px;
    right: -100px;
  }

  .ai-why-choose-us-bg-decoration-{{ ai_gen_id }}:nth-child(2) {
    bottom: -200px;
    left: -100px;
  }

  .ai-why-choose-us-container-{{ ai_gen_id }} {
    max-width: {{ block.settings.container_width }}px;
    margin: 0 auto;
    position: relative;
    z-index: 1;
  }

  .ai-why-choose-us-header-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 70px;
    opacity: 0;
    transform: translateY(30px);
    transition: all 0.8s cubic-bezier(0.16, 1, 0.3, 1);
  }

  .ai-why-choose-us-header-{{ ai_gen_id }}.visible {
    opacity: 1;
    transform: translateY(0);
  }

  .ai-why-choose-us-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    color: {{ block.settings.title_color }};
    margin: 0 0 20px;
    font-weight: 700;
    letter-spacing: -1px;
    line-height: 1.1;
  }

  .ai-why-choose-us-subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.subtitle_size }}px;
    color: {{ block.settings.subtitle_color }};
    margin: 0;
    max-width: 700px;
    margin-left: auto;
    margin-right: auto;
    line-height: 1.6;
    font-weight: 400;
  }

  .ai-why-choose-us-grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));
    gap: 32px;
  }

  .ai-why-choose-us-item-{{ ai_gen_id }} {
    background: {{ block.settings.card_background }};
    padding: 48px 36px;
    border-radius: {{ block.settings.card_radius }}px;
    text-align: center;
    opacity: 0;
    transform: translateY(60px) scale(0.9);
    transition: all 0.6s cubic-bezier(0.16, 1, 0.3, 1);
    box-shadow: 0 8px 30px rgba(0, 0, 0, 0.06);
    position: relative;
    overflow: hidden;
    border: 1px solid rgba(0, 0, 0, 0.04);
  }

  .ai-why-choose-us-item-{{ ai_gen_id }}::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 3px;
    background: linear-gradient(90deg, {{ block.settings.accent_color_1 }}, {{ block.settings.accent_color_2 }});
    transform: scaleX(0);
    transform-origin: left;
    transition: transform 0.6s cubic-bezier(0.16, 1, 0.3, 1);
  }

  .ai-why-choose-us-item-{{ ai_gen_id }}::after {
    content: '';
    position: absolute;
    inset: 0;
    background: linear-gradient(135deg, {{ block.settings.accent_color_1 }}05, {{ block.settings.accent_color_2 }}05);
    opacity: 0;
    transition: opacity 0.4s ease;
  }

  .ai-why-choose-us-item-{{ ai_gen_id }}.visible {
    opacity: 1;
    transform: translateY(0) scale(1);
  }

  .ai-why-choose-us-item-{{ ai_gen_id }}.hidden {
    opacity: 0.3;
    transform: translateY(20px) scale(0.98);
  }

  .ai-why-choose-us-item-{{ ai_gen_id }}:hover {
    transform: translateY(-12px) scale(1.02);
    box-shadow: 0 20px 60px rgba(0, 0, 0, 0.12);
  }

  .ai-why-choose-us-item-{{ ai_gen_id }}.visible:hover::before {
    transform: scaleX(1);
  }

  .ai-why-choose-us-item-{{ ai_gen_id }}:hover::after {
    opacity: 1;
  }

  .ai-why-choose-us-icon-wrapper-{{ ai_gen_id }} {
    width: {{ block.settings.icon_size }}px;
    height: {{ block.settings.icon_size }}px;
    margin: 0 auto 32px;
    background: linear-gradient(135deg, {{ block.settings.accent_color_1 }}, {{ block.settings.accent_color_2 }});
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: all 0.5s cubic-bezier(0.16, 1, 0.3, 1);
    position: relative;
    z-index: 1;
  }

  .ai-why-choose-us-icon-wrapper-{{ ai_gen_id }}::before {
    content: '';
    position: absolute;
    inset: -10px;
    border-radius: 50%;
    background: linear-gradient(135deg, {{ block.settings.accent_color_1 }}, {{ block.settings.accent_color_2 }});
    opacity: 0.15;
    transform: scale(0.8);
    transition: all 0.5s cubic-bezier(0.16, 1, 0.3, 1);
  }

  .ai-why-choose-us-item-{{ ai_gen_id }}.visible:hover .ai-why-choose-us-icon-wrapper-{{ ai_gen_id }} {
    transform: scale(1.15) rotate(8deg);
  }

  .ai-why-choose-us-item-{{ ai_gen_id }}.visible:hover .ai-why-choose-us-icon-wrapper-{{ ai_gen_id }}::before {
    transform: scale(1.1);
    opacity: 0.25;
  }

  .ai-why-choose-us-icon-{{ ai_gen_id }} {
    width: calc({{ block.settings.icon_size }}px * 0.5);
    height: calc({{ block.settings.icon_size }}px * 0.5);
    color: {{ block.settings.icon_color }};
    position: relative;
    z-index: 1;
    stroke-width: 2.5;
  }

  .ai-why-choose-us-item-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.item_title_size }}px;
    color: {{ block.settings.item_title_color }};
    margin: 0 0 16px;
    font-weight: 600;
    letter-spacing: -0.4px;
    position: relative;
    z-index: 1;
  }

  .ai-why-choose-us-item-text-{{ ai_gen_id }} {
    font-size: {{ block.settings.item_text_size }}px;
    color: {{ block.settings.item_text_color }};
    margin: 0;
    line-height: 1.7;
    font-weight: 400;
    position: relative;
    z-index: 1;
  }

