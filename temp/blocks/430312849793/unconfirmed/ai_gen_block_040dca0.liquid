<script>
  (function() {
    class WhyChooseUs{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.header = this.querySelector('[data-animate="header"]');
        this.items = this.querySelectorAll('[data-animate="item"]');
        this.observer = null;
        this.lastScrollY = window.scrollY;
      }

      connectedCallback() {
        this.setupIntersectionObserver();
      }

      setupIntersectionObserver() {
        const options = {
          root: null,
          rootMargin: '-50px 0px -50px 0px',
          threshold: [0, 0.2, 0.5, 0.8]
        };

        this.observer = new IntersectionObserver((entries) => {
          entries.forEach((entry) => {
            const currentScrollY = window.scrollY;
            const isScrollingDown = currentScrollY > this.lastScrollY;
            this.lastScrollY = currentScrollY;

            if (entry.target === this.header) {
              if (entry.isIntersecting) {
                entry.target.classList.add('visible');
              } else {
                entry.target.classList.remove('visible');
              }
            } else {
              if (entry.isIntersecting && isScrollingDown) {
                const delay = entry.target.getAttribute('data-delay');
                setTimeout(() => {
                  entry.target.classList.remove('hidden');
                  entry.target.classList.add('visible');
                }, parseInt(delay));
              } else if (!entry.isIntersecting && !isScrollingDown) {
                entry.target.classList.remove('visible');
                entry.target.classList.add('hidden');
              }
            }
          });
        }, options);

        if (this.header) {
          this.observer.observe(this.header);
        }

        this.items.forEach((item) => {
          this.observer.observe(item);
        });
      }

      disconnectedCallback() {
        if (this.observer) {
          this.observer.disconnect();
        }
      }
    }

    customElements.define('why-choose-us-{{ ai_gen_id }}', WhyChooseUs{{ ai_gen_id }});
  })();
</script>

