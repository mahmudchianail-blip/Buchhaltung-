{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-features-collection-{{ ai_gen_id }} {
    padding: {{ block.settings.section_padding }}px 20px;
    background-color: {{ block.settings.background_color }};
  }

  .ai-features-collection__container-{{ ai_gen_id }} {
    max-width: {{ block.settings.container_width }}px;
    margin: 0 auto;
  }

  .ai-features-collection__header-{{ ai_gen_id }} {
    text-align: {{ block.settings.text_alignment }};
    margin-bottom: {{ block.settings.spacing_below_header }}px;
  }

  .ai-features-collection__title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    color: {{ block.settings.title_color }};
    margin: 0 0 {{ block.settings.spacing_below_title }}px;
    font-weight: 700;
  }

  .ai-features-collection__subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.subtitle_size }}px;
    color: {{ block.settings.subtitle_color }};
    margin: 0;
    opacity: 0.9;
  }

  .ai-features-collection__grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat({{ block.settings.columns_desktop }}, 1fr);
    gap: {{ block.settings.grid_gap }}px;
  }

  .ai-features-collection__item-{{ ai_gen_id }} {
    position: relative;
    overflow: hidden;
    border-radius: {{ block.settings.card_border_radius }}px;
    background-color: {{ block.settings.card_background }};
    box-shadow: {{ block.settings.card_shadow_x }}px {{ block.settings.card_shadow_y }}px {{ block.settings.card_shadow_blur }}px rgba(0, 0, 0, {{ block.settings.card_shadow_opacity }});
    transition: transform 0.4s cubic-bezier(0.4, 0, 0.2, 1), box-shadow 0.4s cubic-bezier(0.4, 0, 0.2, 1);
  }

  .ai-features-collection__item-{{ ai_gen_id }}:hover {
    transform: translateY(-{{ block.settings.hover_lift }}px) scale(1.02);
    box-shadow: {{ block.settings.card_shadow_x | times: 1.5 }}px {{ block.settings.card_shadow_y | times: 2 }}px {{ block.settings.card_shadow_blur | times: 1.5 }}px rgba(0, 0, 0, {{ block.settings.card_shadow_opacity | times: 1.3 }});
  }

  .ai-features-collection__image-wrapper-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    padding-bottom: {{ block.settings.image_aspect_ratio }}%;
    overflow: hidden;
    background-color: #f4f4f4;
  }

  .ai-features-collection__image-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.6s cubic-bezier(0.4, 0, 0.2, 1), opacity 0.6s ease;
  }

  .ai-features-collection__image-{{ ai_gen_id }}.ai-features-collection__image--primary-{{ ai_gen_id }} {
    opacity: 1;
    z-index: 2;
  }

  .ai-features-collection__image-{{ ai_gen_id }}.ai-features-collection__image--secondary-{{ ai_gen_id }} {
    opacity: 0;
    z-index: 1;
  }

  .ai-features-collection__item-{{ ai_gen_id }}:hover .ai-features-collection__image--primary-{{ ai_gen_id }} {
    opacity: 0;
    transform: scale(1.1);
  }

  .ai-features-collection__item-{{ ai_gen_id }}:hover .ai-features-collection__image--secondary-{{ ai_gen_id }} {
    opacity: 1;
    transform: scale(1.1);
  }

  .ai-features-collection__image-placeholder-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #f4f4f4;
  }

  .ai-features-collection__image-placeholder-{{ ai_gen_id }} svg {
    width: 60%;
    height: 60%;
    opacity: 0.3;
  }

  .ai-features-collection__badge-{{ ai_gen_id }} {
    position: absolute;
    top: {{ block.settings.badge_spacing }}px;
    right: {{ block.settings.badge_spacing }}px;
    background-color: {{ block.settings.badge_background }};
    color: {{ block.settings.badge_text_color }};
    padding: 6px 12px;
    border-radius: {{ block.settings.badge_border_radius }}px;
    font-size: {{ block.settings.badge_font_size }}px;
    font-weight: 600;
    z-index: 3;
    opacity: 0;
    transform: translateY(-10px);
    transition: opacity 0.3s ease, transform 0.3s ease;
  }

  .ai-features-collection__item-{{ ai_gen_id }}:hover .ai-features-collection__badge-{{ ai_gen_id }} {
    opacity: 1;
    transform: translateY(0);
  }

  .ai-features-collection__content-{{ ai_gen_id }} {
    padding: {{ block.settings.card_padding }}px;
  }

  .ai-features-collection__icon-wrapper-{{ ai_gen_id }} {
    width: {{ block.settings.icon_size }}px;
    height: {{ block.settings.icon_size }}px;
    background-color: {{ block.settings.icon_background }};
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: {{ block.settings.spacing_below_icon }}px;
    transition: transform 0.4s cubic-bezier(0.4, 0, 0.2, 1), background-color 0.3s ease;
  }

  .ai-features-collection__item-{{ ai_gen_id }}:hover .ai-features-collection__icon-wrapper-{{ ai_gen_id }} {
    transform: rotate(360deg) scale(1.1);
    background-color: {{ block.settings.icon_background_hover }};
  }

  .ai-features-collection__icon-{{ ai_gen_id }} {
    width: {{ block.settings.icon_size | times: 0.5 }}px;
    height: {{ block.settings.icon_size | times: 0.5 }}px;
    color: {{ block.settings.icon_color }};
  }

  .ai-features-collection__feature-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.feature_title_size }}px;
    color: {{ block.settings.feature_title_color }};
    margin: 0 0 {{ block.settings.spacing_below_feature_title }}px;
    font-weight: 600;
    transition: color 0.3s ease;
  }

  .ai-features-collection__item-{{ ai_gen_id }}:hover .ai-features-collection__feature-title-{{ ai_gen_id }} {
    color: {{ block.settings.feature_title_hover_color }};
  }

  .ai-features-collection__description-{{ ai_gen_id }} {
    font-size: {{ block.settings.description_size }}px;
    color: {{ block.settings.description_color }};
    line-height: 1.6;
    margin: 0;
  }

  .ai-features-collection__link-{{ ai_gen_id }} {
    display: inline-flex;
    align-items: center;
    gap: 8px;
    margin-top: {{ block.settings.spacing_above_link }}px;
    color: {{ block.settings.link_color }};
    font-size: {{ block.settings.link_font_size }}px;
    font-weight: 600;
    text-decoration: none;
    transition: gap 0.3s ease, color 0.3s ease;
  }

  .ai-features-collection__link-{{ ai_gen_id }}:hover {
    gap: 12px;
    color: {{ block.settings.link_hover_color }};
  }

  .ai-features-collection__link-arrow-{{ ai_gen_id }} {
    width: 16px;
    height: 16px;
    transition: transform 0.3s ease;
  }

  .ai-features-collection__link-{{ ai_gen_id }}:hover .ai-features-collection__link-arrow-{{ ai_gen_id }} {
    transform: translateX(4px);
  }

  @media screen and (max-width: 989px) {
    .ai-features-collection__grid-{{ ai_gen_id }} {
      grid-template-columns: repeat({{ block.settings.columns_tablet }}, 1fr);
    }
  }

  @media screen and (max-width: 749px) {
    .ai-features-collection__grid-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
    }

    .ai-features-collection__title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.8 }}px;
    }

    .ai-features-collection__subtitle-{{ ai_gen_id }} {
      font-size: {{ block.settings.subtitle_size | times: 0.9 }}px;
    }
  }
{% endstyle %}

<features-collection-{{ ai_gen_id }} class="ai-features-collection-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-features-collection__container-{{ ai_gen_id }}">
    {% if block.settings.title != blank or block.settings.subtitle != blank %}
      <div class="ai-features-collection__header-{{ ai_gen_id }}">
        {% if block.settings.title != blank %}
          <h2 class="ai-features-collection__title-{{ ai_gen_id }}">{{ block.settings.title }}</h2>
        {% endif %}
        {% if block.settings.subtitle != blank %}
          <div class="ai-features-collection__subtitle-{{ ai_gen_id }}">{{ block.settings.subtitle }}</div>
        {% endif %}
      </div>
    {% endif %}

    <div class="ai-features-collection__grid-{{ ai_gen_id }}">
      {% for i in (1..6) %}
        {% liquid
          assign feature_enabled_key = 'feature_' | append: i | append: '_enabled'
          assign feature_title_key = 'feature_' | append: i | append: '_title'
          assign feature_description_key = 'feature_' | append: i | append: '_description'
          assign feature_image_key = 'feature_' | append: i | append: '_image'
          assign feature_image_2_key = 'feature_' | append: i | append: '_image_2'
          assign feature_link_key = 'feature_' | append: i | append: '_link'
          assign feature_link_text_key = 'feature_' | append: i | append: '_link_text'
          assign feature_badge_key = 'feature_' | append: i | append: '_badge'

          assign feature_enabled = block.settings[feature_enabled_key]
          assign feature_title = block.settings[feature_title_key]
          assign feature_description = block.settings[feature_description_key]
          assign feature_image = block.settings[feature_image_key]
          assign feature_image_2 = block.settings[feature_image_2_key]
          assign feature_link = block.settings[feature_link_key]
          assign feature_link_text = block.settings[feature_link_text_key]
          assign feature_badge = block.settings[feature_badge_key]
        %}

        {% if feature_enabled %}
          <div class="ai-features-collection__item-{{ ai_gen_id }}">
            {% if block.settings.show_images %}
              <div class="ai-features-collection__image-wrapper-{{ ai_gen_id }}">
                {% if feature_image != blank %}
                  <img
                    src="{{ feature_image | image_url: width: 800 }}"
                    alt="{{ feature_image.alt | escape }}"
                    loading="lazy"
                    width="800"
                    height="{{ 800 | times: block.settings.image_aspect_ratio | divided_by: 100 }}"
                    class="ai-features-collection__image-{{ ai_gen_id }} ai-features-collection__image--primary-{{ ai_gen_id }}"
                  >
                {% else %}
                  <div class="ai-features-collection__image-placeholder-{{ ai_gen_id }}">
                    {{ 'image' | placeholder_svg_tag }}
                  </div>
                {% endif %}

                {% if feature_image_2 != blank %}
                  <img
                    src="{{ feature_image_2 | image_url: width: 800 }}"
                    alt="{{ feature_image_2.alt | escape }}"
                    loading="lazy"
                    width="800"
                    height="{{ 800 | times: block.settings.image_aspect_ratio | divided_by: 100 }}"
                    class="ai-features-collection__image-{{ ai_gen_id }} ai-features-collection__image--secondary-{{ ai_gen_id }}"
                  >
                {% endif %}

                {% if feature_badge != blank and block.settings.show_badges %}
                  <div class="ai-features-collection__badge-{{ ai_gen_id }}">{{ feature_badge }}</div>
                {% endif %}
              </div>
            {% endif %}

            <div class="ai-features-collection__content-{{ ai_gen_id }}">
              {% if block.settings.show_icons %}
                <div class="ai-features-collection__icon-wrapper-{{ ai_gen_id }}">
                  <svg class="ai-features-collection__icon-{{ ai_gen_id }}" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
                  </svg>
                </div>
              {% endif %}

              {% if feature_title != blank %}
                <h3 class="ai-features-collection__feature-title-{{ ai_gen_id }}">{{ feature_title }}</h3>
              {% endif %}

              {% if feature_description != blank %}
                <div class="ai-features-collection__description-{{ ai_gen_id }}">{{ feature_description }}</div>
              {% endif %}

              {% if feature_link != blank and feature_link_text != blank and block.settings.show_links %}
                <a href="{{ feature_link }}" class="ai-features-collection__link-{{ ai_gen_id }}">
                  {{ feature_link_text }}
                  <svg class="ai-features-collection__link-arrow-{{ ai_gen_id }}" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
                  </svg>
                </a>
              {% endif %}
            </div>
          </div>
        {% endif %}
      {% endfor %}
    </div>
  </div>
</features-collection-{{ ai_gen_id }}>

<script>
  (function() {
    class FeaturesCollection{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.items = this.querySelectorAll('.ai-features-collection__item-{{ ai_gen_id }}');
        this.setupImageRotation();
      }

      setupImageRotation() {
        const autoRotate = {{ block.settings.auto_rotate_images }};
        const rotationInterval = {{ block.settings.rotation_interval }} * 1000;

        if (!autoRotate) return;

        this.items.forEach((item) => {
          const primaryImage = item.querySelector('.ai-features-collection__image--primary-{{ ai_gen_id }}');
          const secondaryImage = item.querySelector('.ai-features-collection__image--secondary-{{ ai_gen_id }}');

          if (!primaryImage || !secondaryImage) return;

          let isShowingPrimary = true;

          setInterval(() => {
            if (isShowingPrimary) {
              primaryImage.style.opacity = '0';
              primaryImage.style.transform = 'scale(1.1)';
              secondaryImage.style.opacity = '1';
              secondaryImage.style.transform = 'scale(1.1)';
            } else {
              primaryImage.style.opacity = '1';
              primaryImage.style.transform = 'scale(1)';
              secondaryImage.style.opacity = '0';
              secondaryImage.style.transform = 'scale(1)';
            }
            isShowingPrimary = !isShowingPrimary;
          }, rotationInterval);
        });
      }
    }

    customElements.define('features-collection-{{ ai_gen_id }}', FeaturesCollection{{ ai_gen_id }});
  })();
</script>

